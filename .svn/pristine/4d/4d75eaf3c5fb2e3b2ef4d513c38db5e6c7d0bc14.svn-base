plate>\r\n                </el-table-column>\r\n              </el-table>\r\n            </el-tab-pane>\r\n            <el-tab-pane label=\"記錄列表\" name=\"second\">\r\n              <el-table\r\n                border\r\n                :data=\"billList\"\r\n                style=\"width: 100%\"\r\n                @row-click=\"delBill\"\r\n                @row-dblclick=\"updateBill\"\r\n                v-loading=\"loading\"\r\n                element-loading-text=\"加载中...\"\r\n                element-loading-spinner=\"el-icon-loading\"\r\n              >\r\n                <el-table-column align=\"center\" prop=\"time\" label=\"日期\">\r\n                  <template slot-scope=\"scope\">\r\n                    <div\r\n                      :style=\"{\r\n                        color:\r\n                          isRed(scope.row.flag, scope.row.totalHkPrice) == 1\r\n                            ? 'red'\r\n                            : '#606266'\r\n                      }\"\r\n                    >\r\n                      {{ scope.row.time }}\r\n                    </div>\r\n                  </template>\r\n                </el-table-column>\r\n                <el-table-column\r\n                  width=\"150px\"\r\n                  align=\"center\"\r\n                  prop=\"tradeType\"\r\n                  label=\"買入/賣出\"\r\n                >\r\n                  <template slot-scope=\"scope\">\r\n                    <div\r\n                      :style=\"{\r\n                        color:\r\n                          isRed(scope.row.flag, scope.row.totalHkPrice) == 1\r\n                            ? 'red'\r\n                            : '#606266'\r\n                      }\"\r\n                    >\r\n                      {{\r\n                        isOther == true &&\r\n                        (tradeTypeRgx(scope.row.tradeType) == \"買入\" ||\r\n                          tradeTypeRgx(scope.row.tradeType) == \"賣出\")\r\n                          ? scope.row.companyName +\r\n                            \" \" +\r\n                            tradeTypeRgx(scope.row.tradeType)\r\n                          : tradeTypeRgx(scope.row.tradeType)\r\n                      }}\r\n                    </div>\r\n                  </template>\r\n                </el-table-column>\r\n                <el-table-column align=\"center\" prop=\"stockList\" label=\"貨號\">\r\n                  <template slot-scope=\"scope\">\r\n                    <div\r\n                      :style=\"{\r\n                        color:\r\n                          isRed(scope.row.flag, scope.row.totalHkPrice) == 1\r\n                            ? 'red'\r\n                            : '#606266'\r\n                      }\"\r\n                      v-html=\"productCodeGet(scope.row.stockList)\"\r\n                    ></div>\r\n                  </template>\r\n                </el-table-column>\r\n                <el-table-column\r\n                  width=\"250px\"\r\n                  align=\"center\"\r\n                  prop=\"productDes\"\r\n                  label=\"產品描述\"\r\n                >\r\n                  <template slot-scope=\"scope\">\r\n                    <div\r\n                      :style=\"{\r\n                        color:\r\n                          isRed(scope.row.flag, scope.row.totalHkPrice) == 1\r\n                            ? 'red'\r\n                            : '#606266'\r\n                      }\"\r\n                    >\r\n                      <el-tooltip effect=\"light\" place=\"top-start\">\r\n                        <div\r\n                          v-html=\"\r\n                            scope.row.productDes.replace(/[\\r\\n]/g, '<br />')\r\n                          \"\r\n                          slot=\"content\"\r\n                        ></div>\r\n                        <div\r\n                          class=\"font-warp\"\r\n                          v-html=\"\r\n                            scope.row.productDes.r