tip\r\n                        class=\"item\"\r\n                        effect=\"light\"\r\n                        :content=\"scope.row.remark\"\r\n                        placement=\"top-start\"\r\n                      >\r\n                        <div class=\"font-warp\">{{ scope.row.remark }}</div>\r\n                      </el-tooltip>\r\n                    </div>\r\n                  </template>\r\n                </el-table-column>\r\n                <el-table-column align=\"center\" label=\"操作\">\r\n                  <template>\r\n                    <div>\r\n                      <span style=\"color:#409EFF;cursor: pointer;\">刪除</span>\r\n                    </div>\r\n                  </template>\r\n                </el-table-column>\r\n              </el-table>\r\n              <div style=\"margin-top:15px;text-align: right\">\r\n                <el-pagination\r\n                  @current-change=\"handleCurrentChange\"\r\n                  :current-page=\"page\"\r\n                  layout=\"total, prev, pager, next, jumper\"\r\n                  :total=\"total\"\r\n                >\r\n                </el-pagination>\r\n              </div>\r\n            </el-tab-pane>\r\n          </el-tabs>\r\n        </div>\r\n        <div v-show=\"isCompany == 1\">\r\n          <div\r\n            class=\"right-title\"\r\n            v-if=\"personAllMoney !== 0 && isOther == false\"\r\n          >\r\n            公司欠\r\n            {{ personName + \"總數： \" }}\r\n            <span\r\n              :style=\"{\r\n                color: personAllMoney < 0 ? 'red' : '#000'\r\n              }\"\r\n              >{{ formatNumberRgx(personAllMoney)\r\n              }}<span style=\"margin-left: 5px;font-size: 12px;\">{{\r\n                personAllCurrency\r\n              }}</span></span\r\n            >\r\n          </div>\r\n          <div\r\n            class=\"right-title\"\r\n            v-if=\"personAllMoney !== 0 && isOther == true\"\r\n          >\r\n           