ect=\"light\"\r\n                      :content=\"scope.row.remark\"\r\n                      placement=\"top-end\"\r\n                    >\r\n                      <div class=\"font-warp\">{{ scope.row.remark }}</div>\r\n                    </el-tooltip>\r\n                  </div>\r\n                </template>\r\n              </el-table-column>\r\n            </el-table>\r\n          </el-form-item>\r\n        </el-form>\r\n      </el-tab-pane>\r\n      <el-tab-pane\r\n        :label=\"sold == 8 ? '寄卖信息' : '销售信息'\"\r\n        name=\"third\"\r\n        v-if=\"sold == 2 || sold == 3 || sold == 8\"\r\n      >\r\n        <el-form ref=\"saleForm\" label-width=\"121px\">\r\n          <el-form-item\r\n            label=\"账单号\"\r\n            :required=\"isStock == 1 || sold == 8 ? true : false\"\r\n            v-if=\"sold == 3 || sold == 8\"\r\n          >\r\n            <el-input v-model=\"bill\" style=\"width: 50%;\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item\r\n            :label=\"sold == 8 ? '寄卖时间' : '出售时间'\"\r\n            :required=\"isStock == 1 || sold == 8 ? true : false\"\r\n            v-if=\"sold == 3 || sold == 8\"\r\n          >\r\n            <el-date-picker\r\n              v-model=\"soldTime\"\r\n              type=\"date\"\r\n              placeholder=\"请选择日期\"\r\n              value-format=\"yyyy-MM-dd\"\r\n              format=\"yyyy-MM-dd\"\r\n              style=\"width:50%;\"\r\n            ></el-date-picker>\r\n          </el-form-item>\r\n          <el-form-item :label=\"sold == 8 ? '寄卖外币金额' : '出售外币金额'\">\r\n            <div style=\"width: 50%;display: flex;\">\r\n              <el-input\r\n                type=\"text\"\r\n                style=\"flex: 1;\"\r\n                :placeholder=\"\r\n                  sold == 8 ? '请输入寄卖外币金额' : '请输入出售外币金额'\r\n                \"\r\n                v-model=\"priceTran\"\r\n                clearable\r\n                @change=\"isSellHKD\"\r\n              ></el-input>\r\n     