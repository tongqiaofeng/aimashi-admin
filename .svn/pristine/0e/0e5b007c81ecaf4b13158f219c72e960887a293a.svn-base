\n                  <el-input\r\n                    style=\"flex: 1;\"\r\n                    v-model=\"headSellMoney\"\r\n                    :placeholder=\"\r\n                      '请输入出售' +\r\n                        currencyFontRgx(headCurrency) +\r\n                        '金额，用于与总公司结算'\r\n                    \"\r\n                  ></el-input>\r\n                </div>\r\n              </el-form-item>\r\n              <el-form-item\r\n                label=\"销售人员\"\r\n                :required=\"sold == 3 ? false : true\"\r\n              >\r\n                <el-select\r\n                  style=\"width: 100%;\"\r\n                  v-model=\"sellerId\"\r\n                  placeholder=\"请选择销售人员\"\r\n                >\r\n                  <el-option\r\n                    v-for=\"item in sellerList\"\r\n                    :key=\"item.id\"\r\n                    :label=\"item.name\"\r\n                    :value=\"item.id\"\r\n                  >\r\n                  </el-option>\r\n                </el-select>\r\n              </el-form-item>\r\n              <el-form-item\r\n                label=\"客户姓名\"\r\n                :required=\"sold == 3 ? false : true\"\r\n              >\r\n                <el-autocomplete\r\n                  style=\"width: 100%;\"\r\n                  v-model=\"customer\"\r\n                  :fetch-suggestions=\"querySearch\"\r\n                  placeholder=\"请选择/输入客户姓名\"\r\n                  @select=\"handleSelect\"\r\n                ></el-autocomplete>\r\n              </el-form-item>\r\n              <el-form-item\r\n                label=\"客户类型\"\r\n                :required=\"sold == 3 ? false : true\"\r\n              >\r\n                <el-autocomplete\r\n                  style=\"width: 100%;\"\r\n                  v-model=\"customerType\"\r\n                  :fetch-suggestions=\"queryCustomerTypeSearch\"\r\n                  placeholder=\"请选择/输入客户类型\"\r\n                  @select=\"handleCustomerTypeSelect\"\r\n                ></el-autocomplete>\r\n              </el-form-item>\r\n              <el-form-item label=\"出库时间\" required v-if=\"sold == 4\">\r\n                <el-date-picker\r\n                  v-model=\"stockOutTime\"\r\n                  type=\"date\"\r\n                  style=\"width:100%;\"\r\n                  placeholder=\"请选择日期\"\r\n                  value-format=\"yyyy-MM-dd\"\r\n                  format=\"yyyy-MM-dd\"\r\n                ></el-date-picker>\r\n              </el-form-item>\r\n              <el-form-item label=\"备注\">\r\n                <el-input\r\n                  type=\"textarea\"\r\n                  style=\"width:100%;\"\r\n                  v-model=\"note\"\r\n                ></el-input>\r\n              </el-form-item>\r\n              <el-form-item\r\n                label=\"出售记录\"\r\n                v-show=\"sellPaymentList.length > 0\"\r\n              >\r\n                <el-table :data=\"sellPaymentList\" style=\"width: 100%;\">\r\n                  <el-table-column\r\n                    align=\"center\"\r\n                    prop=\"time\"\r\n                    label=\"日期\"\r\n                    width=\"200px\"\r\n                  >\r\n                    <template slot-scope=\"scope\">\r\n                      <div>\r\n                        <div>\r\n                          {{ scope.row.time }}\r\n                        </div>\r\n                        <div>\r\n                          {{ \"【\" + scope.row.personName + \"】\" }}\r\n                        </div>\r\n                      </div>\r\n                    </template>\r\n                  </el-table-column>\r\n                  <el-table-column\r\n                    width=\"200px\"\r\n                    align=\"center\"\r\n                    prop=\"productDes\"\r\n                    label=\"產品描述\"\r\n                  >\r\n                    <template slot-scope=\"scope\">\r\n                      <div>\r\n                        <el-tooltip\r\n                          class=\"item\"\r\n                          effect=\"light\"\r\n                          :content=\"scope.row.productDes\"\r\n                          placement=\"top-end\"\r\n                        >\r\n                          <div class=\"font-warp\">\r\n                            {{ scope.row.productDes }}\r\n                          </div>\r\n                        </el-tooltip>\r\n                      </div>\r\n                    </template>\r\n                  </el-table-column>\r\n                  <el-table-column align=\"center\" prop=\"money\" label=\"外幣金額\">\r\n                    <template slot-scope=\"scope\">\r\n                      <div>\r\n                        {{\r\n                          scope.row.money == \"\" || scope.row.money == 0\r\n                            ? \"/\"\r\n                            : formatNumberRgx(scope.row.money) +\r\n                              \" \" +\r\n                              scope.row.currency\r\n                        }}\r\n                      </div>\r\n                    </template>\r\n                  </el-table-column>\r\n                  <el-table-column\r\n                    align=\"center\"\r\n                    prop=\"totalToHkRate\"\r\n                    label=\"匯率\"\r\n                  >\r\n                    <template slot-scope=\"scope\">\r\n                      <div>\r\n                        {{\r\n                          scope.row.totalToHkRate == \"\" ||\r\n                          scope.row.totalToHkRate == 0\r\n                            ? \"/\"\r\n                            : scope.row.totalToHkRate\r\n                        }}\r\n                      </div>\r\n                    </template>\r\n                  </el-table-column>\r\n                  <el-table-column\r\n                    align=\"center\"\r\n                    prop=\"totalHkPrice\"\r\n                    :label=\"currencyFontRgx(currencyGlobal) + '金額'\"\r\n                  >\r\n                    <template slot-scope=\"scope\">\r\n                      <div>\r\n                        {{\r\n                          scope.row.totalHkPrice == \"\" ||\r\n                          scope.row.totalHkPrice == 0\r\n                            ? \"/\"\r\n                            : formatNumberRgx(scope.row.totalHkPrice) +\r\n                              \" \" +\r\n                              currencyGlobal\r\n                        }}\r\n                      </div>\r\n                    </template>\r\n                  </el-table-column>\r\n                  <el-table-column\r\n                    align=\"center\"\r\n                    prop=\"receiveType\"\r\n                    label=\"交易方式\"\r\n                  >\r\n                  </el-table-column>\r\n                  <el-table-column\r\n                    width=\"200px\"\r\n                    align=\"center\"\r\n                    prop=\"remark\"\r\n                    label=\"Remarks\"\r\n                  >\r\n                    <template slot-scope=\"scope\">\r\n                      <div>\r\n                        <el-tooltip\r\n                          class=\"item\"\r\n                          effect=\"light\"\r\n                          :content=\"scope.row.remark\"\r\n                          placement=\"top-end\"\r\n                        >\r\n                          <div class=\"font-warp\">{{ scope.row.remark }}</div>\r\n                        </el-tooltip>\r\n                      </div>\r\n                    </template>\r\n                  </el-table-column>\r\n                </el-table>\r\n              </el-form-item>\r\n            </el-form>\r\n          </div>\r\n          <div slot=\"footer\">\r\n            <el-button @click=\"dialogStateVisible = false\">取 消</el-button>\r\n            <el-button type=\"primary\" @click=\"stateupdateSure\"\r\n              >确 定\r\n            </el-button>\r\n          </div>\r\n        </el