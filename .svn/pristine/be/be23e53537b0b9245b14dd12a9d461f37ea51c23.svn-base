             v-model=\"customer\"\r\n                  :fetch-suggestions=\"querySearch\"\r\n                  placeholder=\"请选择/输入客户姓名\"\r\n                  @select=\"handleSelect\"\r\n                ></el-autocomplete>\r\n              </el-form-item>\r\n              <el-form-item\r\n                label=\"客户类型\"\r\n                :required=\"isStock == 1 || sold == 8 ? true : false\"\r\n                v-if=\"sold == 3 || sold == 8\"\r\n              >\r\n                <el-autocomplete\r\n                  style=\"width: 100%;\"\r\n                  v-model=\"customerType\"\r\n                  :fetch-suggestions=\"queryCustomerTypeSearch\"\r\n                  placeholder=\"请选择/输入客户类型\"\r\n                  @select=\"handleCustomerTypeSelect\"\r\n                ></el-autocomplete>\r\n              </el-form-item>\r\n              <el-form-item label=\"接收仓库\" v-show=\"sold == 8\" required>\r\n                <el-cascader\r\n                  style=\"width: 100%;\"\r\n                  v-model=\"receiveWarehouseId\"\r\n                  :options=\"companyAndWarehouseList\"\r\n                  :props=\"{\r\n                    value: 'id',\r\n                    label: 'name',\r\n                    children: 'warehouseList'\r\n                  }\"\r\n                ></el-cascader>\r\n              </el-form-item>\r\n              <el-form-item\r\n                label=\"是否同时出库\"\r\n                label-width=\"110px\"\r\n                v-if=\"sold == 3\"\r\n              >\r\n                <el-radio-group v-model=\"isStock\">\r\n                  <el-radio :label=\"0\">否</el-radio>\r\n                  <el-radio :label=\"1\">是</el-radio>\r\n                </el-radio-group>\r\n              </el-form-item>\r\n              <el-form-item\r\n                label=\"出库时间\"\r\n                required\r\n                v-if=\"sold == 3 && isStock == 1\"\r\n              >\r\n                <el-date-picker\r\n                  v-model=\"stockOutTime\"\r\n                  type=\"date\"\r\n                  style=\"width:100%;\"\r\n                  placeholder=\"请选择日期\"\r\n                  value-format=\"yyyy-MM-dd\"\r\n                  format=\"yyyy-MM-dd\"\r\n                ></el-date-picker>\r\n              </el-form-item>\r\n              <el-form-item label=\"备注\">\r\n                <el-input\r\n                  type=\"textarea\"\r\n                  style=\"width:100%;\"\r\n                  v-model=\"note\"\r\n                ></el-input>\r\n              </el-form-item>\r\n              <el-form-item\r\n                label=\"出售记录\"\r\n                v-show=\"salePaymentList.length > 0 && sold == 3\"\r\n              >\r\n                <el-table :data=\"salePaymentList\" style=\"width: 100%;\">\r\n                  <el-table-column\r\n                    align=\"center\"\r\n                    prop=\"time\"\r\n                    label=\"日期\"\r\n                    width=\"200px\"\r\n                  >\r\n                    <template slot-scope=\"scope\">\r\n                      <div>\r\n                        <div>\r\n                          {{ scope.row.time }}\r\n                        </div>\r\n                        <div>\r\n                          {{ \"【\" + scope.row.personName + \"】\" }}\r\n                        </div>\r\n                      </div>\r\n                    </template>\r\n                  </el-table-column>\r\n                  <el-table-column\r\n                    width=\"200px\"\r\n                    align=\"center\"\r\n                    prop=\"productDes\"\r\n                    label=\"產品描述\"\r\n                  >\r\n                    <template slot-scope=\"scope\">\r\n                      <div>\r\n                        <el-tooltip\r\n                          class=\"item\"\r\n                          effect=\"light\"\r\n                          :content=\"scope.row.productDes\"\r\n                          placement=\"top-end\"\r\n         