\n            :label=\"sold == 8 ? '寄卖时间' : '出售时间'\"\r\n            :required=\"isStock == 1 || sold == 8 ? true : false\"\r\n            v-if=\"sold == 3 || sold == 8\"\r\n          >\r\n            <el-date-picker\r\n              v-model=\"soldTime\"\r\n              type=\"date\"\r\n              placeholder=\"请选择日期\"\r\n              value-format=\"yyyy-MM-dd\"\r\n              format=\"yyyy-MM-dd\"\r\n              style=\"width:50%;\"\r\n            ></el-date-picker>\r\n          </el-form-item>\r\n          <el-form-item :label=\"sold == 8 ? '寄卖外币金额' : '出售外币金额'\">\r\n            <div style=\"width: 50%;display: flex;\">\r\n              <el-input\r\n                type=\"text\"\r\n                style=\"flex: 1;\"\r\n                :placeholder=\"\r\n                  sold == 8 ? '请输入寄卖外币金额' : '请输入出售外币金额'\r\n                \"\r\n                v-model=\"priceTran\"\r\n                clearable\r\n                @change=\"isSellHKD\"\r\n              ></el-input>\r\n              <el-select\r\n                v-model=\"sellCurrencyId\"\r\n                placeholder=\"请选择\"\r\n                clearable\r\n                @change=\"isSellHKD\"\r\n              >\r\n                <el-option\r\n                  v-for=\"item in currencyIds\"\r\n                  :key=\"item.value\"\r\n                  :label=\"item.label\"\r\n                  :value=\"item.value\"\r\n                >\r\n                </el-option>\r\n              </el-select>\r\n            </div>\r\n          </el-form-item>\r\n          <el-form-item label=\"是否收款完成\" v-if=\"sold == 3 || sold == 8\">\r\n            <el-switch v-model=\"isReceiveCheck\"></el-switch>\r\n          </el-form-item>\r\n          <!-- 物流费/银行手续费送货(HKD) -->\r\n          <el-form-item label=\"物流费/手续费\" v-if=\"sold == 3 || sold == 8\">\r\n            <div style=\"width: 50%;display: flex;\">\r\n              <el-input\r\n                style=\"flex: 1;\"\r\n                v-model=\"saleLogHkPrice\"\r\n    