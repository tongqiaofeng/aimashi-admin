eholder=\"請選擇\"\r\n              clearable\r\n              @change=\"personIdIsSam\"\r\n            >\r\n              <el-option\r\n                v-for=\"(item, index) in userList\"\r\n                :key=\"index\"\r\n                :label=\"item.name\"\r\n                :value=\"item.id\"\r\n              ></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item\r\n            label=\"收款賬戶\"\r\n            v-show=\"billData.tradeType == 2\"\r\n            prop=\"payeeId\"\r\n          >\r\n            <el-select\r\n              style=\"width:100%;\"\r\n              v-model=\"billData.payeeId\"\r\n              placeholder=\"請選擇\"\r\n              @change=\"payeeIdIsSam\"\r\n            >\r\n              <el-option\r\n                v-for=\"(item, index) in userList\"\r\n                :key=\"index\"\r\n                :label=\"item.name\"\r\n                :value=\"item.id\"\r\n                v-show=\"item.id != billData.personId\"\r\n              ></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item label=\"交易方式\" prop=\"receiveType\">\r\n            <el-autocomplete\r\n              style=\"width: 100%;\"\r\n              class=\"inline-input\"\r\n              v-model=\"billData.receiveType\"\r\n              :fetch-suggestions=\"querySearch\"\r\n              placeholder=\"請選擇/輸入內容\"\r\n              @select=\"handleSelect\"\r\n            ></el-autocomplete>\r\n          </el-form-item>\r\n          <el-form-item\r\n            label=\"產品貨號\"\r\n            prop=\"productCode\"\r\n            v-show=\"accountNumberType == 1\"\r\n          >\r\n            <div class=\"code-table\">\r\n              <div class=\"every1\">\r\n                <span style=\"color: red;\">*</span>\r\n                貨號\r\n              </div>\r\n              <div class=\"every2\">\r\n                外幣金額\r\n              </div>\r\n              <div class=\"every3\">外幣轉{{ companyCurr